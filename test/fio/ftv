# 100mb verify test 

[global]
ioengine=io_uring
iodepth=16
direct=1
filename=/dev/lsvbd1
numjobs=1
verify=pattern
verify_pattern=0xAA
verify_state_save=1

[write_test]
rw=write
size=100M
bssplit=4k/100
do_verify=0
verify_fatal=0
verify_only=0

[read_verify_test]
rw=read
size=100M
bssplit=8k/100
do_verify=0
verify_fatal=0
verify_only=1

